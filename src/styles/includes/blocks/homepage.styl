.homepage
	height 100%
	
	&__wrapper
		display grid
		grid-template-columns 100%
		grid-template-rows auto 1fr
		gap 128px
		width 740px
		max-width 100%
		height 100%
		margin-right auto
		margin-left auto
		padding 75px 20px
		// padding 170px 20px
		// padding 170px 20px
		
		@media $1440-
			gap 80px
			padding-top 45px
			padding-bottom 45px
		
		// @media $smMinus
		// 	padding-top 86px
	
	&__header
		text-align center
	
	&__logo
		vertical-align middle
	
	&__title
		margin-bottom 40px
		font-weight 600
		font-size 34px
		text-align center