extends layouts/user_profile

block vars
  - userProfileClasses = 'user-profile--vacancies';

block content
  .vacancies
    h1.page-title.vacancies__title Вакансії
    .filter.vacancies__filter
      .filter__top
        .filter__item
          input(type="search" name="query" placeholder="Назва вакансії...").form-text.form-text--filter-search
        .filter__item.hidden-xxs
          select(name="countries" autocomplete="off").form-select.form-select--filter#countries
            option(value="") Усі країни
            option(value="poland") Польща
            option(value="czech") Чехія
        .filter__item.hidden-xxs
          select(name="cities").form-select.form-select--filter#cities
            option(value="") Усі міста
            option(value="city_1") Місто 1
            option(value="city_2") Місто 2
            option(value="city_3") Місто 3
            option(value="city_4") Місто 4
        .filter__item.hidden-xxs
          input(type="number" name="income_from" placeholder="Дохід від, грн").form-text.form-text--filter
        .filter__item.hidden-xxs
          select(name="currency").form-select.form-select--filter#currency
            option(value="") Валюти
            option(value="uah") UAH
            option(value="pln") PLN
            option(value="eur") EUR
        .filter__item.filter__item--buttons.hidden-xxs
          button(type="button" data-fancybox data-src="#additional-filters-popup").btn-white.btn-white--filter.additional-filters-popup-link Фільтри
          button(type="button").btn-success.filter__search-btn Знайти
      .filter__bottom
        button(type="button" data-fancybox data-src="#additional-filters-popup").btn-white.btn-white--filter.hidden-xxsPlus.additional-filters-popup-link Фільтри
        .filter-tags.filter-tags--job-filter
          ul.filter-tags__list
            li.filter-tags__item
              button(type="button").filter-tag Без досвіду
            li.filter-tags__item
              button(type="button").filter-tag Спеціаліст
            li.filter-tags__item
              button(type="button").filter-tag Чоловіки
            li.filter-tags__item
              button(type="button").filter-tag Жінки
            li.filter-tags__item
              button(type="button").filter-tag Сімейні пари
        a(href="javascript:;").clear-link.filter__clear-link.hidden-xxs Скинути фільтри
    .vacancy-cards.vacancies__cards
      .vacancy-card.vacancy-cards__item
        .vacancy-card__header
          .vacancy-info.vacancy-card__info
            .vacancy-info__logo
              img(src="/img/lg_electronics.svg" alt="LG Electronics").vacancy-info__logo-img
            .vacancy-info__top
              .vacancy-info__company-name LG Electronics
              .tag.vacancy-info__category Автозавод
              a(href="#" data-bookmark).bookmark-icon.vacancy-info__bookmark-icon
            .vacancy-info__specialization Виготовлення електроніки та різних складових
          .vacancy-card__rating-and-address
            //- .rating
            //-   .rating__wrapper
            //-     .rating__mark 8.8
            //-     .rating__total 10
            //-   .rating__label (198 відгуків)
            .vacancy-card__address Poland, Gorzów Wielkopolski, Złotego Smoka 9, 66-400
        h2.vacancy-card__title Оператор на виробництво електроніки
        .vacancy-card__specifications
          .specification.specification--large.vacancy-card__specification
            .specification__title Оплата за годину
            .specification__value 18 – 20 zl / год
          .specification.specification--large.vacancy-card__specification
            .specification__title Відпрацювання
            .specification__value 500 – 800 zl
          .specification.specification--large.vacancy-card__specification
            .specification__title Оплата за міс
            .specification__value 3500 – 4900 zl / міс
          .specification.specification--large.vacancy-card__specification
            .specification__title Термін
            .specification__value 30 днів
          .specification.vacancy-card__specification.vacancy-card__specification--responsibilities
            .specification__title Обов'язки 
            .specification__value
              p Комплектація замовлень Комплектація замовлень Комплектація замовлень Комплектація замовлень за допомогою сканера та електровузика пакування товару в коробки, палетування а інші
        .promo-blocks.vacancy-card__promo-blocks.hidden-smPlus
          .promo-blocks-swiper.swiper
            .swiper-wrapper
              .swiper-slide.promo-blocks-swiper__slide
                .promo-block
                  .promo-block__header
                    .promo-block__logo
                      img(src="/img/white_work_company.png" alt="White Work Company").promo-block__logo-img
                    h3.promo-block__title White Work Company
                  .characteristics.promo-block__characteristics
                    .characteristic.characteristic--sex.characteristics__item
                      .characteristic__value Чоловіки, жінки, сімейні пари
                    .characteristic.characteristic--age.characteristics__item
                      .characteristic__value до 55 років
                    .characteristic.characteristic--schedule.characteristics__item
                      .characteristic__value Пн–Пт, 2 зміни по 8–12 год
                    .characteristic.characteristic--house.characteristics__item
                      .characteristic__value Безкоштовне, 300 зл/міс комуналь
                  .salary.promo-block__salary #[span.salary__value 800 zl] винагорода
                  .promo-block__footer
                    a(href="/vacancy.html" target="_blank").more-link.more-link--full-width.promo-block__more-link Детальніше
